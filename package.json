{
  "name": "meteora-damm-bot",
  "version": "0.1.0",
  "description": "TypeScript implementation of Meteora DAMM V2 liquidity bot using official Meteora SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "__start": "node dist/index.js",
    "start": "ts-node src/app/index.ts",
    "dev": "ts-node src/index.ts",
    "monitor-migrations": "LIVE=0 ts-node src/monitor-migrations.ts",
    "test": "ts-node ./test.ts",
    "test:dry-run": "LIVE=0 ts-node src/test.ts",
    "test:live": "LIVE=1 ts-node src/test.ts",
    "test:liquidity": "LIVE=0 ts-node src/test.ts liquidity",
    "test:position": "LIVE=0 ts-node src/test.ts position",
    "test:single": "LIVE=0 ts-node src/test.ts single",
    "test:scan": "ts-node src/test.ts scan",
    "test:graduation": "ts-node src/test.ts graduation",
    "test:watcher": "ts-node src/test.ts watcher",
    "test:websocket": "ts-node src/test.ts websocket",
    "test:check-pool": "ts-node src/test.ts check-pool",
    "test:missed-migration": "ts-node src/test.ts missed-migration",
    "test:deep-scan": "ts-node src/test.ts deep-scan",
    "monitor": "ts-node src/test.ts websocket",
    "integrated-flow": "ts-node src/test.ts integrated-flow",
    "clean": "rm -rf dist",
    "watch": "tsc --watch"
  },
  "keywords": [
    "solana",
    "meteora",
    "damm",
    "defi",
    "liquidity",
    "typescript",
    "zap-sdk"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@jup-ag/api": "^6.0.44",
    "@meteora-ag/cp-amm-sdk": "file:damm-v2-sdk-full",
    "@meteora-ag/zap-sdk": "^1.0.7",
    "@solana/spl-token": "^0.3.9",
    "bs58": "^5.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.98.4",
    "@types/bs58": "^4.0.1",
    "@types/node": "^20.0.0",
    "bottleneck": "^2.19.5",
    "dotenv": "^17.2.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
